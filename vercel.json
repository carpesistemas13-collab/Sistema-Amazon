{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["backend/dist/**", "backend/src/**", "backend/.env.example"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/api/index.js"
    }
  ]
}